{
    "appearance": {
        "extraBackgroundTint": true,
        "fakeScreenRounding": 1,
        "palette": {
            "type": "scheme-tonal-spot"
        },
        "transparency": {
            "automatic": true,
            "backgroundTransparency": 0.11,
            "contentTransparency": 0.57,
            "enable": true
        },
        "wallpaperTheming": {
            "enableAppsAndShell": true,
            "enableQtApps": true,
            "enableTerminal": true
        }
    },
    "apps": {
        "bluetooth": "kcmshell6 kcm_bluetooth",
        "network": "nm-connection-editor",
        "networkEthernet": "nm-connection-editor",
        "taskManager": "gnome-system-monitor",
        "terminal": "kitty -1"
    },
    "audio": {
        "protection": {
            "enable": true,
            "maxAllowed": 100,
            "maxAllowedIncrease": 10
        }
    },
    "background": {
        "clockX": -500,
        "clockY": -500,
        "fixedClockPosition": true,
        "parallax": {
            "enableSidebar": true,
            "enableWorkspace": true,
            "workspaceZoom": 1.07
        },
        "thumbnailPath": "",
        "wallpaperPath": ""
    },
    "bar": {
        "autoHide": {
            "enable": false,
            "pushWindows": false,
            "showWhenPressingSuper": {
                "delay": 140,
                "enable": true
            }
        },
        "borderless": false,
        "bottom": false,
        "cornerStyle": 1,
        "multiMonitorMode": false,
        "primaryMonitor": "HDMI-A-1",
        "resources": {
            "alwaysShowCpu": false,
            "alwaysShowSwap": true
        },
        "screenList": [
        ],
        "showBackground": true,
        "topLeftIcon": "spark",
        "tray": {
            "blacklistedApps": [
                "spotify-client",
                "electron"
            ],
            "monochromeIcons": true
        },
        "utilButtons": {
            "showColorPicker": false,
            "showDarkModeToggle": false,
            "showKeyboardToggle": false,
            "showMicToggle": false,
            "showPerformanceProfileToggle": false,
            "showScreenSnip": false
        },
        "verbose": true,
        "weather": {
            "city": "BSB",
            "enable": true,
            "enableGPS": false,
            "fetchInterval": 10,
            "useUSCS": false
        },
        "workspaces": {
            "alwaysShowNumbers": false,
            "monochromeIcons": true,
            "showAppIcons": true,
            "showNumberDelay": 300,
            "shown": 5
        }
    },
    "battery": {
        "automaticSuspend": true,
        "critical": 5,
        "low": 20,
        "suspend": 3
    },
    "dock": {
        "enable": false,
        "height": 60,
        "hoverRegionHeight": 3,
        "hoverToReveal": false,
        "ignoredAppRegexes": [
        ],
        "monochromeIcons": true,
        "pinnedApps": [
            "org.kde.dolphin",
            "kitty"
        ],
        "pinnedOnStartup": false
    },
    "hacks": {
        "arbitraryRaceConditionDelay": 20
    },
    "interactions": {
        "scrolling": {
            "fasterTouchpadScroll": true,
            "mouseScrollDeltaThreshold": 120,
            "mouseScrollFactor": 120,
            "touchpadScrollFactor": 450
        }
    },
    "language": {
        "translator": {
            "engine": "auto",
            "sourceLanguage": "auto",
            "targetLanguage": "auto"
        }
    },
    "light": {
        "night": {
            "automatic": true,
            "colorTemperature": 4000,
            "from": "18:00",
            "to": "06:30"
        }
    },
    "media": {
        "filterDuplicatePlayers": true
    },
    "osd": {
        "timeout": 1000
    },
    "osk": {
        "layout": "qwerty_full",
        "pinnedOnStartup": false
    },
    "overview": {
        "columns": 5,
        "enable": true,
        "rows": 2,
        "scale": 0.1
    },
    "privacy": {
        "webcamDisabled": true
    },
    "resources": {
        "updateInterval": 100
    },
    "screenshotTool": {
        "showContentRegions": true
    },
    "search": {
        "engineBaseUrl": "https://www.google.com/search?q=",
        "excludedSites": [
            "quora.com"
        ],
        "nonAppResultDelay": 30,
        "prefix": {
            "action": "/",
            "clipboard": ";",
            "emojis": ":"
        },
        "sloppy": false
    },
    "sidebar": {
        "keepRightSidebarLoaded": true,
        "translator": {
            "delay": 300
        }
    },
    "time": {
        "dateFormat": "dddd, dd/MM",
        "format": "hh:mm",
        "pomodoro": {
            "alertSound": "",
            "breakTime": 300,
            "cyclesBeforeLongBreak": 4,
            "focus": 1500,
            "longBreak": 900
        }
    },
    "windows": {
        "centerTitle": true,
        "showTitlebar": false
    }
}
